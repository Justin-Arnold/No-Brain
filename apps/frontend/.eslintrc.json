{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@nuxtjs/eslint-config-typescript",
        "plugin:prettier/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "rules": {
        "@typescript-eslint/no-explicit-any": "error"
    }
}
